{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.6.*",
        "barryvdh/laravel-debugbar": "^3.1",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "davestewart/sketchpad": "^1.2",
        "barryvdh/laravel-cors": "^0.9.3",
        "spatie/laravel-fractal": "^5.0",
        "tymon/jwt-auth": "^1.0@dev",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Jenky\\LaravelGenerators\\": "packages/jenky/laravel-generators/src/",
            "Jenky\\LaravelAPI\\": "packages/jenky/laravel-api/src/",
            "Jenky\\RESTfulEloquent\\": "packages/jenky/restful-eloquent/src/",
            "Jenky\\LaravelPushNotification\\": "packages/jenky/laravel-push-notification/src/",
            "Jenky\\SmartRule\\": "packages/jenky/smart-rule/src/",
            "Jenky\\LaravelPlupload\\": "packages/jenky/laravel-plupload/src/",
            "JenKy\\ValidationPresets\\": "packages/jenky/laravel-validation-presets/src/",
            "sketchpad\\": "sketchpad/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
